<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="20dp"
    tools:ignore="Deprecated" >

    <TextView
        android:id="@+id/lableDenomination"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="1dp"
        android:layout_x="10dp"
        android:layout_y="3dp"
        android:textSize="12sp"
        android:text="@string/cashier_model_lable_denomination_number"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/lablesalescountnumber"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="1dp"
        android:layout_x="40dp"
        android:layout_y="5dp"
        android:gravity="right"
        android:text="@string/cashier_model_lable_Sales_card_count_number"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/lablesalesamountnumber"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:layout_x="90dp"
        android:layout_y="5dp"
        android:gravity="right"
        android:text="@string/cashier_model_lable_Sales_card_mount_number"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/lableInhandcountnumber"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="1dp"
        android:layout_x="185dp"
        android:layout_y="5dp"
        android:gravity="right"
        android:text="@string/cashier_model_lable_inhand_card_count_number"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/lableInhandamountnumber"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:gravity="center"
        android:text="@string/cashier_model_lable_inhand_card_mount_number"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="11sp" />

</LinearLayout>