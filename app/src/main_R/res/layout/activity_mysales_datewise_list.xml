<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:paddingTop="4dip"
     android:paddingBottom="6dip"
     android:layout_width="fill_parent"
     android:layout_height="wrap_content"
     android:orientation="horizontal"
       android:background="#FFFFFF" >
    
    
    <RelativeLayout 
        
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        
        >
        
        <AbsoluteLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true" >

<TextView
    android:id="@+id/lstDateMySalesColumn5"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="5dp"
    android:layout_marginTop="5dp"
    android:layout_weight="2"
    android:layout_x="271dp"
    android:layout_y="9dp"
    android:hint="Date"
    android:textColor="#000000"
    android:textStyle="bold" />

<TextView
    android:id="@+id/lstDenommySalesColumn3"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="5dp"
    android:layout_marginTop="5dp"
    android:layout_weight="2"
    android:layout_x="141dp"
    android:layout_y="6dp"
    android:hint="Denom"
    android:textColor="#000000"
    android:textStyle="bold" />

<TextView
    android:id="@+id/lstQtymySalesColumn4"
    android:layout_width="36dp"
    android:layout_height="wrap_content"
    android:layout_marginBottom="5dp"
    android:layout_marginTop="5dp"
    android:layout_weight="2"
    android:layout_x="218dp"
    android:layout_y="8dp"
    android:hint="Qty"
    android:textColor="#000000"
    android:textStyle="bold" />

<TextView
    android:id="@+id/lstMerchantMySalesColumn1"
    android:layout_width="74dp"
    android:layout_height="wrap_content"
    android:layout_marginBottom="5dp"
    android:layout_marginTop="5dp"
    android:layout_weight="2"
    android:layout_x="4dp"
    android:layout_y="8dp"
    android:hint="Merchant"
    android:paddingLeft="10dp"
    android:textColor="#000000"
    android:textStyle="bold" />

<TextView
    android:id="@+id/lstCardmySalesColumn2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_weight="2"
    android:layout_x="90dp"
    android:layout_y="8dp"
    android:hint="Card"
    android:textColor="#000000"
    android:textStyle="bold" />

</AbsoluteLayout>

</RelativeLayout>
    

</LinearLayout>